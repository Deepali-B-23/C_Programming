#include<stdio.h>

int MultFact(int iNo)
{
    int i = 0;
    if(iNo<=0)
    {
        iNo = -iNo;
    }
    printf("1");
    for(i=2;i<=iNo;i++)
    {
        if(iNo % i==0)
        {
            printf("%d",i);
        }
    }
    return iNo;
 
}
int main()
{
    int iValue = 0;
    int iRet = 0;

    printf("Enter number\n ");
    scanf("%d",&iValue);

    iRet = MultFact(iValue);



    return 0;
}
