#include<stdio.h>

void PrintEven(int iNo)
{
    int i = 1;
    if(iNo <= 0)
    {
        
       return;
    }
    while(iNo > 0)
    {
        printf("%d\n",i*2);
        i++;
        iNo--;
    }
}
int main()
{
    int iValue = 0;
    printf("Enter Number\n");
    scanf("%d",&iValue);
    PrintEven(iValue);
    
    return 0;
}
